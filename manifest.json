{
    "name": "MathFlare",
    "short_name": "MathFlare",
    "description": "the go-to open source website for geometry and algebra",
    "author": "MathFlare Co. Ltd.",
    "version": "3,6",
    "manifest_version": 1,
    "offline_enabled": false,
    "default_locale": "en",
    "lang": "English",
    "start_url": "/",
    "homepage_url": "https://www.mathflare.tk",
    "icons": [
        {
            "src": "/Assets/favicon.ico",
            "sizes": "128x128"
        },
        {
            "src": "/Assets/apple-touch-icon.png",
            "sizes": "180x180"
        },
        {
            "src": "/Assets/android-chrome-192x192.png",
            "sizes": "192x192"
        },
        {
            "src": "/Assets/android-chrome-512x512.png",
            "sizes": "512x512"
        }
    ],
    "theme_color": "#FFC107",
    "background_color": "#23272B",
    "display": "standalone",
    "orientation": "portrait"
